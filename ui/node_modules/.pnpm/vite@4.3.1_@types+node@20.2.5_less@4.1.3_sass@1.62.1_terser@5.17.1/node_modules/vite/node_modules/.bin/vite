#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aleks/code/neo-atlas/node_modules/.pnpm/vite@4.3.1_@types+node@20.2.5_less@4.1.3_sass@1.62.1_terser@5.17.1/node_modules/vite/bin/node_modules:/home/aleks/code/neo-atlas/node_modules/.pnpm/vite@4.3.1_@types+node@20.2.5_less@4.1.3_sass@1.62.1_terser@5.17.1/node_modules/vite/node_modules:/home/aleks/code/neo-atlas/node_modules/.pnpm/vite@4.3.1_@types+node@20.2.5_less@4.1.3_sass@1.62.1_terser@5.17.1/node_modules:/home/aleks/code/neo-atlas/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/aleks/code/neo-atlas/node_modules/.pnpm/vite@4.3.1_@types+node@20.2.5_less@4.1.3_sass@1.62.1_terser@5.17.1/node_modules/vite/bin/node_modules:/home/aleks/code/neo-atlas/node_modules/.pnpm/vite@4.3.1_@types+node@20.2.5_less@4.1.3_sass@1.62.1_terser@5.17.1/node_modules/vite/node_modules:/home/aleks/code/neo-atlas/node_modules/.pnpm/vite@4.3.1_@types+node@20.2.5_less@4.1.3_sass@1.62.1_terser@5.17.1/node_modules:/home/aleks/code/neo-atlas/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
